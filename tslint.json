{
  "extends": "tslint:latest",
  "rules": {
    "curly": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "arrow-parens": false,
    "trailing-comma": [
      true,
      {
        "multiline": "true",
        "singleline": "never"
      }
    ],
    "indent": [
      true,
      "spaces"
    ]
  }
}
