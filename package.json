  "devDependencies": {
    "@types/mocha": "^2.2.35",
    "@types/supertest": "^2.0.0",
    "cz-conventional-changelog": "^1.2.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "gulp-help": "^1.6.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^4.0.0",
    "gulp-tar-path": "^1.0.1",
    "gulp-tslint": "^7.0.1",
    "gulp-typescript": "^3.1.4",
    "husky": "^0.14.2",
    "merge2": "^1.0.3",
    "semantic-release": "^6.3.2",
    "standard": "^10.0.1",
    "supertest": "^3.0.0",
    "ts-node": "^3.0.1",
    "tslint": "~4.5.0",
    "typescript": "^2.1.4"
  },
  "engines": {
    "node": ">=4.2.0"
